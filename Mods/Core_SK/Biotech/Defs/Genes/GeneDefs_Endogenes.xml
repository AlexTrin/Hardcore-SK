	<!-- HSK Unique Genes -->
	
  <GeneTemplateDef ParentName="GeneAptitudeBase">
    <defName>AptitudeBelowAverage</defName>
    <label>below average {0}</label>
    <description>The carrier's aptitude in {0} is reduced by 2. Aptitude acts like an offset on skill level.</description>
    <iconPath>UI/Icons/Genes/Skills/{0}/Poor</iconPath>
    <aptitudeOffset>-2</aptitudeOffset>
    <displayOrderOffset>10</displayOrderOffset>
    <biostatCpx>1</biostatCpx>
    <biostatMet>0</biostatMet>
  </GeneTemplateDef>

  <GeneTemplateDef ParentName="GeneAptitudeBase">
    <defName>AptitudeAboveAverage</defName>
    <label>above average {0}</label>
    <description>The carrier's aptitude in {0} is increased by 2. Aptitude acts like an offset on skill level.</description>
    <iconPath>UI/Icons/Genes/Skills/{0}/Strong</iconPath>
    <aptitudeOffset>2</aptitudeOffset>
    <displayOrderOffset>20</displayOrderOffset>
    <biostatCpx>1</biostatCpx>
    <biostatMet>0</biostatMet>
  </GeneTemplateDef>
	
  <GeneDef ParentName="GeneToxResistBase">
    <defName>ToxResist_Minor</defName>
    <label>minor tox resistance</label>
    <labelShortAdj>tox-resistant</labelShortAdj>
    <description>Carriers of this gene are slightly resistant to toxic buildup from any source. This includes pollution, toxic fallout, tox gas, and direct attacks with venom or injected poison. They'll gain half the amount of toxic buildup compared to others.\n\nCellular filters in the lung and skin reduce the dose of toxins entering the bloodstream.</description>
    <iconPath>UI/Icons/Genes/Gene_MinorToxicityResistance</iconPath>
    <displayOrderInCategory>0</displayOrderInCategory>
    <statOffsets>
      <ToxicResistance>0.25</ToxicResistance>
    </statOffsets>
    <biostatMet>-1</biostatMet>
    <symbolPack>
      <prefixSymbols>
        <li>
          <symbol>tox</symbol>
          <weight>3</weight>
        </li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>

  <GeneDef ParentName="GeneToxResistBase">
    <defName>GermResist_Minor</defName>
    <label>minor germ resistance</label>
    <labelShortAdj>germ-resistant</labelShortAdj>
    <description>Carriers of this gene are slightly resistant to germs from any source. This includes any diseases.</description>
    <iconPath>UI/Icons/Genes/Gene_GermResistMinor</iconPath>
    <displayOrderInCategory>0</displayOrderInCategory>
    <statOffsets>
      <GermResistance>0.05</GermResistance>
    </statOffsets>
    <biostatMet>0</biostatMet>
    <symbolPack>
      <prefixSymbols>
        <li>
          <symbol>tox</symbol>
          <weight>3</weight>
        </li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>

